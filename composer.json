{
    "name": "artisan-build/kibble",
    "description": "",
    "keywords": ["packages", "composer", "console", "cli"],
    "homepage": "https://community.artisan.build",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/artisan-build/kibble/issues",
        "source": "https://github.com/artisan-build/kibble"
    },
    "authors": [
        {
            "name": "Artisan Build",
            "email": "hello@artisan.build"
        }
    ],
    "require": {
        "php": "^8.3.0",
        "laravel-zero/framework": "^11.0.2",
        "laravel/prompts": "^0.3.2"
    },
    "require-dev": {
        "laravel/pint": "^1.18.1",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.5.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["kibble"]
}
